<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

    <servlet>
        <servlet-name>ModsServiceProvider</servlet-name>
        <servlet-class>cz.mzk.modseditordemo.server.ModsServiceProvider</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ModsServiceProvider</servlet-name>
        <url-pattern>/ModsEditorDemo/mods</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>ModsEditorDemo.html</welcome-file>
    </welcome-file-list>

</web-app>
